<h2>List of ratings</h2>

<p>Number of ratings is <%= @ratings.count %></p>
<ul>
 <% @ratings.each do |rating| %>
   <li> <%= rating.beer.name %> <%= rating %> <%= link_to 'delete', rating, :method => :delete, data: { confirm: 'Are you sure?' }  %> </li>
 <% end %> 
 

</ul>

<%= link_to 'Add new rating', new_rating_path  %>
